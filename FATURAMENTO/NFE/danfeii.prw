		If MV_PAR05 == 1 .And. nFolhas == 2
			nLenItens -= nMaxI2						
		Else
			nLenItens -= nMaxIAll
		EndIf
		if ( nLenItens > -10 .And. nLenItens < 0 ) .And. nFolhas > 1//Coloca mais uma folha para impressao do rodape
			nFolhas++
		endif
	Else
		if ( nLenItens > -10 .And. nLenItens < 0 ) .And. nFolhas == 1//Coloca mais uma folha para impressao do rodape
			nFolhas++
		endif
		lFlag := .F.
