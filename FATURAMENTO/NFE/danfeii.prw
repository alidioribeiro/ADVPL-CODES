				If !Empty(aXML[nX][2]) .And. Empty((cAliasSFT)->FT_DTCANC).AND. (AllTrim((cAliasSFT)->FT_ESPECIE)== 'SPED') .Or. (lImpDir .And. !Empty(aXML[nX][2]))
