//³ SetPrvt, que criara somente as variaveis definidas pelo usuario,    ³
//³ identificando as variaveis publicas do sistema utilizadas no codigo ³
//³ Incluido pelo assistente de conversao do AP5 IDE                    ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

SetPrvt("XALIAS,CRECNO,XCONTA,")

// MATA01.PRX -> EXECBLOCK PARA GERACAO AUTOMATICA DAS CONTAS DE DESPESAS.

xAlias := Alias()
cRecno := Recno()
xConta := "" 

if IsTelnet()

Else
Do Case
     Case AllTrim(m->cCC) == "211"  // M O N T A G E M
          If AllTrim(SB1->B1_GRUPO)     == "MPI"  // MPN - Mat. Prima Importado
             xConta := "32111001"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MPN"  // MPN - Mat. Prima Nacional
             xConta := "32111002"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSI"  // MSI - Mat. secundario importado.
             xConta := "32111003"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSN"  // MSN - Mat. secundario nacional.
             xConta := "32111004"
          End
     
     Case AllTrim(m->cCC) == "221"  // I  M  P  R  E  S  S  Ã  O
          If AllTrim(SB1->B1_GRUPO)     == "MPI"  // MPN - Mat. Prima Importado
             xConta := "32112001"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MPN"  // MPN - Mat. Prima Nacional
             xConta := "32112002"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSI"  // MSI - Mat. secundario importado.
             xConta := "32112003"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSN"  // MSN - Mat. secundario nacional.
             xConta := "32112004"
          End
     
     Case AllTrim(m->cCC) == "231"  // I N J E Ç Ã O
          If     AllTrim(SB1->B1_GRUPO) == "MPI"  // MPN - Mat. Prima Importado
             xConta := "32114001"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MPN"  // MPN - Mat. Prima Nacional
             xConta := "32114002"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSI"  // MSI - Mat. secundario importado.
             xConta := "32114003"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSN"  // MSN - Mat. secundario nacional.
             xConta := "32114004"
          END

     Case AllTrim(m->cCC) == "241"  // I N S E R C A O   A U T O M A T I C A
          If AllTrim(SB1->B1_GRUPO)     == "MPI"  // MPN - Mat. Prima Importado
             xConta := "15111003"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MPN"  // MPN - Mat. Prima Nacional
             xConta := "15111003"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSI"  // MSI - Mat. secundario importado.
             xConta := "15111003"
          ElseIf AllTrim(SB1->B1_GRUPO) == "MSN"  // MSN - Mat. secundario nacional.
             xConta := "15111003"
          End          
          
     OTHERWISE
          If cTM == "600"
              MSGSTOP("O Centro de Custo ["+AllTrim(cCC)+"] não é um Centro de Trabalho. POR FAVOR CORRIGIR CC Centro de Custo.")
          End
EndCase
Endif
Return(xConta)
