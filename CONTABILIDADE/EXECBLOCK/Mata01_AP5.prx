User Function Mata01a()
Local lRet   := .T.
Local c_nPrg := FunName()

//If c_nPrg == "MATA241" .and. xFilial("SB1")="01"
  If xFilial("SD3")=="01"
	If c_nPrg == "MATA241"   
	    If cTM == "600" .And. AllTrim(SB1->B1_Tipo) == "PA" .And. AllTrim(SB1->B1_ASTEC) <> "S"
			MSGSTOP("Tipo de Movimetação [600] para PA só pode ser feita em ASTEC. Caso contrário utilizar Tipo de Movimentação [610]. POR FAVOR CORRIGIR TM - TIPO DE MOVIMENTO.")
			lRet:=.F.
		Elseif cTM == "600" .And. !(AllTrim(SB1->B1_Tipo) $ "PA/MP/PI/RT/BN") 
			MSGSTOP("Tipo de Movimetação [600] é somente para MP,PI ou PA(ASTEC). Requisição de Materiais Improdutivos, usar Tipo de Movimentação [610]. POR FAVOR CORRIGIR TM - TIPO DE MOVIMENTO.")
			lRet:=.F.
		ElseIf cTM == "610" .And. (AllTrim(SB1->B1_Tipo) $ "MP/PI/RT/BN")
			MSGSTOP("Tipos de Materiais iguais a MP ou PI, devem usuar Tipo de Movimetação [600].") 
		ElseIf cTM == "630" .And. !(AllTrim(SB1->B1_Tipo) $ "MC")
			MSGSTOP("Tipo de Movimetação [630] deve movimentar somente materiais do tipo [MC].") 
		End
	EndIf
 EndIF	
Return(lRet)
